{
	"name" : "Webcomic Keyboard Navigation",
	"description" : "Adds keyboard navigation to various webcomics.",
	"version" : "0.5",
	"permissions" : [
		"storage"
	],
	"options_page" : "settings.html",
	"background" : {
		"scripts" : ["install.js"],
		"persistent" : false
	},
	"content_scripts" : [{
		"matches" : ["<all_urls>"],
		"js" : [
			"wckf.js",
			"jQuery.js"
		],
		"run_at" : "document_idle",
		"all_frames" : false
	}],
	"icons": {
		"48": "icon-48.png",
		"128": "icon-128.png"
	},
	"page_action": {
		"default_title": "Keyboard Navigation is enabled!",
		"default_icon": {
			"19": "icon-19.png",
			"38": "icon-38.png"
		}
	},
	"manifest_version": 2,
	"key":""
}
