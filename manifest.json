{
	"name" : "__MSG_extName__",
	"description" : "__MSG_extDesc__",
	"version" : "0.6",
	"default_locale" : "en",
	"permissions" : [
		"storage",
		"notifications",
		"tabs"
	],
	"options_page" : "settings.html",
	"background" : {
		"scripts" : ["install.js"],
		"persistent" : false
	},
	"content_scripts" : [{
		"matches" : ["<all_urls>"],
		"js" : [
			"wckf.js",
			"jQuery.js"
		],
		"run_at" : "document_idle",
		"all_frames" : false
	}],
	"icons": {
		"48": "icon-48.png",
		"128": "icon-128.png"
	},
	"page_action": {
		"default_title": "__MSG_extIconHover__",
		"default_icon": {
			"19": "icon-19.png",
			"38": "icon-38.png"
		}
	},
	"manifest_version": 2,
	"key":""
}
